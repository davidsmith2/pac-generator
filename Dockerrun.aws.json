{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "assets",
      "host": {
        "sourcePath": "/var/app/current/assets"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "pac-generator-server",
      "image": "davidsmith2/pac-generator-server",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8081,
          "containerPort": 8081
        }
      ]
    },
    {
      "name": "pac-generator-proxy",
      "image": "davidsmith2/pac-generator-proxy",
      "essential": false,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "pac-generator-server"
      ],
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-pac-generator-proxy",
          "containerPath": "/var/log/nginx"
        },
        {
          "sourceVolume": "assets",
          "containerPath": "/mnt"
        }
      ]
    }
  ]
}