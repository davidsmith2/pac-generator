{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "pac-generator-server",
      "image": "davidsmith2/pac-generator-server:test1",
      "portMappings": [
        {
          "hostPort": 8081,
          "containerPort": 8081
        }
      ]
    },
    {
      "name": "pac-generator-client",
      "image": "davidsmith2/pac-generator-client:test1",
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "pac-generator-server"
      ]
    }
  ]  
}