{
  "AWSEBDockerrunVersion": 2,
  "volumes": [],
  "containerDefinitions": [
    {
      "name": "pac-generator-server",
      "image": "davidsmith2/pac-generator-server:test3",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 8081,
          "containerPort": 8081
        }
      ]
    },
    {
      "name": "pac-generator-client",
      "image": "davidsmith2/pac-generator-client:test6",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "pac-generator-server"
      ],
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-pac-generator-client",
          "containerPath": "/var/log/nginx"
        }
      ]
    }
  ]
}